{
  "name": "@mdsol/lego",
  "version": "3.1.2",
  "description": "MCC-platform components",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "semistandard && npm run karma",
    "fix": "semistandard --fix",
    "build": "../../script/build.sh",
    "clean": "rm -rf lib/ && rm -rf doc",
    "build-files": "webpack --config=config/webpack.config.js",
    "typings-file": "react2dts --top-level-module",
    "storybook": "../../node_modules/.bin/start-storybook -p 9001 -c .storybook",
    "build-doc": "build-storybook -c .storybook -o doc/",
    "deploy-doc": "aws s3 sync doc/ s3://mdsol-resources/lego/doc/${npm_package_version} --acl public-read",
    "release:s3": "npm pack | xargs -I '{}' aws s3 cp '{}' s3://mdsol-resources/lego/{} --grants read=uri=http://acs.amazonaws.com/groups/global/AllUsers",
    "update-readme": "node script/update-readme.js"
  },
  "publishConfig": {
    "registry": "https://artv4.imedidata.net/artifactory/api/npm/medidata-design-system"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdsol/lego.git"
  },
  "author": "Max Polun <maxpolun@gmail.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/mdsol/lego/issues"
  },
  "homepage": "https://github.com/mdsol/lego#readme",
  "peerDependencies": {
    "jquery": "*",
    "prop-types": ">=15.5.0",
    "react": ">=15.0.0",
    "react-bootstrap": ">=0.30",
    "react-dom": ">=15.0.0"
  },
  "dependencies": {
    "react-block-ui": "^0.2.4",
    "react-datepicker": "^0.53.0",
    "react-proptype-conditional-require": "^1.0.4"
  },
  "semistandard": {
    "ignore": [
      "src/index.js",
      "lib/**",
      "**/util/svgAssets.js"
    ],
    "globals": [
      "jasmine",
      "beforeEach",
      "afterEach",
      "describe",
      "it",
      "fit",
      "expect",
      "spyOn"
    ]
  }
}
