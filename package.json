{
  "name": "components",
  "version": "1.1.0",
  "description": "just a place to put components I make",
  "author": "Richard Kichenama <rkichenama@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rkichenama/components.git"
  },
  "bugs": {
    "url": "https://github.com/rkichenama/components/issues"
  },
  "homepage": "https://github.com/rkichenama/components#readme",
  "main": "index.js",
  "scripts": {
    "check-types": "tsc",
    "buildtsc": "tsc --build ./tsconfig.json",
    "worker:loader": "babel ./app/loadWorker.js -o ./docs/loader.js --minified",
    "worker:worker": "babel ./app/worker.js -o ./docs/worker.js --minified",
    "clear:lib": "del-cli './lib/*' '!./lib/package.json'",
    "clear:doc": "del-cli './docs/*' '!./docs/media'",
    "clear": "run-p -l clear:*",
    "metadata:build": "node doc-gen.js",
    "prebuild": "run-s -l clear test metadata:build ; true",
    "make:pre": "webpack --config ./config/webpack-pre.config.js",
    "build:worker": "run-p -l worker:*",
    "build:docs": "webpack --config ./config/webpack-prod.config.js",
    "prebuild:lib": "run-s buildtsc",
    "build:lib": "webpack --config ./config/webpack-pack.config.js",
    "build": "run-p -l build:*",
    "dev:server": "webpack-dev-server --config ./config/webpack-devl.config.js --color --open",
    "dev": "run-p dev:server watch",
    "test": "jest --config=./config/jest.json",
    "test:ci": "jest --config=./config/jest.json --runInBand --ci",
    "test:watch": "jest --watch --config=./config/jest.json --reporters 'default' --coverageReporters 'text' --color",
    "lint:src": "eslint --ext .jsx,.js src/ --color --format table --fix",
    "lint:app": "eslint --ext .jsx,.js app/ --color --format table --fix",
    "lint:ts": "eslint --config ./.tslintrc --ext .tsx,.ts src/ --color --format table --fix",
    "lint": "run-s -c lint:*",
    "watch:medidata": "chokidar './src/**/*.scss' './src/**/*.jsx' './src/**/*.tsx' './src/**/*.md' -c 'npm run metadata:build'",
    "watch:tests": "jest --watchAll --config=./config/jest.json --color",
    "watch": "run-p -l watch:*"
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-syntax-highlighter": "^10.2.1",
    "@typescript-eslint/parser": "^1.10.2",
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.1",
    "cpy": "^7.3.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-class-property": "^1.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "jest-junit": "^6.4.0",
    "react-docgen-typescript": "^1.12.4",
    "refactor": "^0.0.3",
    "refractor": "2.9.0",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.15",
    "ansi-to-html": "^0.6.11",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-typescript-to-proptypes": "^0.17.1",
    "chokidar-cli": "^1.2.2",
    "css-loader": "^3.0.0",
    "del-cli": "^2.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "hygen": "^4.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-canvas-mock": "^2.1.0",
    "jest-cli": "^24.8.0",
    "jest-json-reporter": "^1.2.2",
    "mini-css-extract-plugin": "^0.7.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prop-types": "^15.7.2",
    "raw-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-docgen": "^4.1.1",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.0.8",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-syntax-highlighter": "^10.3.0",
    "react-to-typescript-definitions": "^2.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "upath": "1.1.2",
    "url-loader": "^2.0.0",
    "webpack": "^4.34.0",
    "webpack-command": "^0.4.2"
  },
  "eslintIgnore": [
    "**/__mocks__/",
    "app/",
    "node_modules/",
    ".circleci/",
    "docs/",
    "lib/"
  ]
}
