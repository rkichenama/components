  "main": "index.js",
  "scripts": {
    "clear": "del-cli './docs/*' '!./docs/media'",
    "prebuild": "npm run clear ; npm run test ; true",
    "build": "webpack --config ./config/webpack-prod.config.js",
    "dev:server": "webpack-dev-server --config ./config/webpack-devl.config.js --open --color",
    "metadata:build": "node doc-gen.js",
    "metadata:watch": "chokidar './src/**/*.{js|jsx|md}' './app/examples/**/*.{js|jsx}' -c 'npm run metadata:build'",
    "dev": "run-p -l metadata:watch dev:server test:watchAll",
    "test": "jest --config=./config/jest.json",
    "test:watchAll": "jest --watchAll --config=./config/jest.json --color",
    "test:watch": "jest --watch --config=./config/jest.json --reporters 'default' --coverageReporters 'text' --color"
  },
