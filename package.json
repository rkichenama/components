{
  "name": "components",
  "version": "1.1.0",
  "description": "just a place to put components I make",
  "author": "Richard Kichenama <rkichenama@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rkichenama/components.git"
  },
  "bugs": {
    "url": "https://github.com/rkichenama/components/issues"
  },
  "homepage": "https://github.com/rkichenama/components#readme",
  "main": "index.js",
  "scripts": {
    "worker:loader": "babel ./app/loadWorker.js -o ./docs/loader.js --minified",
    "worker:worker": "babel ./app/worker.js -o ./docs/worker.js --minified",
    "worker": "run-p -l worker:*",
    "clear:lib": "del-cli './lib/*' '!./lib/package.json'",
    "clear:doc": "del-cli './docs/*' '!./docs/media'",
    "clear": "run-p -l clear:*",
    "metadata:watch": "chokidar './src/**/*.{jsx|md}' -c 'npm run metadata:build'",
    "metadata:build": "node doc-gen.js",
    "prebuild": "run-s -l clear test metadata:build ; true",
    "make:pre": "webpack --config ./config/webpack-pre.config.js --progress",
    "build:docs": "webpack --config ./config/webpack-prod.config.js --progress",
    "build:lib": "webpack --config ./config/webpack-pack.config.js --progress",
    "build": "run-p -l build:* worker",
    "dev:server": "webpack-serve --config ./config/webpack-devl.config.js --open --color",
    "dev": "run-p -l metadata:watch dev:server test:watchAll",
    "test": "jest --config=./config/jest.json",
    "test:ci": "jest --config=./config/jest.json --runInBand --ci",
    "test:watchAll": "jest --watchAll --config=./config/jest.json --color",
    "test:watch": "jest --watch --config=./config/jest.json --reporters 'default' --coverageReporters 'text' --color",
    "lint": "eslint --ext .jsx,.js src/ --color"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-class-property": "^1.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jest-junit": "^5.2.0"
  },
  "devDependencies": {
    "ansi-to-html": "^0.6.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chokidar-cli": "^1.2.0",
    "css-loader": "^0.28.11",
    "del-cli": "^1.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "hygen": "^1.5.9",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.0.0",
    "jest-canvas-mock": "^1.0.2",
    "jest-cli": "^23.0.0",
    "jest-json-reporter": "^1.2.2",
    "mini-css-extract-plugin": "^0.4.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^4.0.1",
    "prerender-loader": "^1.0.0",
    "prop-types": "^15.6.1",
    "raw-loader": "^0.5.1",
    "react": "^16.4.0",
    "react-docgen": "^2.20.1",
    "react-dom": "^16.4.0",
    "react-markdown": "^3.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-syntax-highlighter": "^7.0.4",
    "react-to-typescript-definitions": "^1.0.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "upath": "1.1.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-command": "^0.2.0",
    "webpack-serve": "^1.0.2",
    "webpack-serve-waitpage": "^1.0.0"
  },
  "eslintIgnore": [
    "**/__mocks__/",
    "app/",
    "node_modules/",
    ".circleci/",
    "docs/",
    "lib/"
  ]
}
