{
  "name": "components",
  "version": "1.1.0",
  "description": "just a place to put components I make",
  "author": "Richard Kichenama <rkichenama@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rkichenama/components.git"
  },
  "bugs": {
    "url": "https://github.com/rkichenama/components/issues"
  },
  "homepage": "https://github.com/rkichenama/components#readme",
  "main": "index.js",
  "scripts": {
    "worker:loader": "babel ./app/loadWorker.js -o ./docs/loader.js --minified && true",
    "worker:worker": "babel ./app/worker.js -o ./docs/worker.js --minified && true",
    "worker": "run-p -l worker:*",
    "clear:lib": "del-cli './lib/*' '!./lib/package.json'",
    "clear:doc": "del-cli './docs/*' '!./docs/media'",
    "clear": "run-p -l clear:*",
    "metadata:watch": "chokidar './src/**/*.{jsx|md}' -c 'npm run metadata:build'",
    "metadata:build": "node doc-gen.js && true",
    "prebuild": "run-s -l clear test metadata:build ; true",
    "make:pre": "webpack --config ./config/webpack-pre.config.js --progress",
    "build:docs": "webpack --config ./config/webpack-prod.config.js --progress",
    "build:lib": "webpack --config ./config/webpack-pack.config.js --progress",
    "build": "run-p -l build:*",
    "dev:server": "webpack-serve --config ./config/webpack-devl.config.js --open --color",
    "dev": "run-p -l metadata:watch dev:server test:watchAll",
    "test": "jest --config=./config/jest.json",
    "test:ci": "jest --config=./config/jest.json --runInBand --ci",
    "test:watchAll": "jest --watchAll --config=./config/jest.json --color",
    "test:watch": "jest --watch --config=./config/jest.json --reporters 'default' --coverageReporters 'text' --color",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-react-jsx": "^7.12.17",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@types/axios": "^0.14.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.21",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "axios": "^0.21.1",
    "babel-jest": "^26.6.3",
    "babel-preset-env": "^1.7.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "jest-junit": "^12.0.0",
    "lodash": "^4.17.21",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "ansi-to-html": "^0.6.14",
    "chokidar-cli": "^2.1.0",
    "css-loader": "^5.1.3",
    "del-cli": "^3.0.1",
    "enzyme": "^3.11.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "hygen": "^6.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.1",
    "jest-cli": "^26.6.3",
    "jest-json-reporter": "^1.2.2",
    "mini-css-extract-plugin": "^1.3.9",
    "moxios": "^0.4.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prerender-loader": "^1.3.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.2",
    "react": "^17.0.1",
    "react-docgen": "^5.3.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^5.0.3",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "^15.4.3",
    "react-to-typescript-definitions": "^2.0.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "upath": "2.0.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.26.3",
    "webpack-command": "^0.5.0",
    "webpack-serve": "^3.2.0",
    "webpack-serve-waitpage": "^1.0.2"
  },
  "eslintIgnore": [
    "**/__mocks__/",
    "app/",
    "node_modules/",
    ".circleci/",
    "docs/",
    "lib/"
  ]
}
