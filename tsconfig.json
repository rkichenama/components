{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true
  },
  "include": [
    "./src/**/*.tsx"
  ],
  "exclude": [
    "./node_modules/**/*"
  ]
}